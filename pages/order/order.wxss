.order-list {
  margin: 26rpx 0;
  background: #fff;
  border-radius:9rpx;
  background:rgba(255,255,255,1);
}
.price-image {
  width: 36rpx;
  height: 38rpx;
  margin-right: 12rpx;
}
.list-time-status {
  display: flex;
  justify-content: space-between;
  margin-bottom: 37rpx;
  width: 100%;
  height: 92rpx;
  border-bottom: 1px solid #ebebeb;
}
.list-time {
  margin: auto;
  margin-left: 46rpx;
  font-size:28rpx;
 color:#b8b8b8;
}
.list-status {
  margin: auto;
  margin-right: 32rpx;
  font-size:28rpx;
  color:#b8b8b8;
}
.list-total {
  margin: auto;
  margin-right: 32rpx;
  font-size:28rpx;
  color:#b8b8b8;
}
.list-total text {
  margin: 0 10rpx;
  font-size:30rpx;
  font-weight:bold;
  color:rgba(89,219,157,1);
}
.list-localtion {
  display: flex;
  margin-bottom: 36rpx;
}
.circle {
  width:5px;
  height:5px;
  background:rgba(89,219,157,1);
  border-radius:50%;
  margin: auto 10rpx auto 46rpx;
}
.list-location-text {
  width: 85%;
  font-size: 32rpx;
  color: #333;
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
}
.list-info {
  padding-bottom: 36rpx;
}
.child_list {
  display: flex;
  margin-bottom: 24rpx;
}
.child_list view {
  font-size:28rpx;
  color:#999;
}
.type-name {
  width: 35%;
  margin-left: 65rpx;
  margin-right: 0;
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
}
.type-weight {
  width: 35%;
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
}
.type-price {
  display: flex;
}
.type-price text {
  color:#9be9c4;
  font-weight: bold;
}
.data-empty{
  margin:50% 0 0 0;
}
.data-empty .noOrder_con image{
  width:240rpx;
  height:238rpx;
}
.noorder_text{
  font-size:36rpx;
  line-height: 88rpx;
  color:rgba(98,223,163,.6);
  text-align:center;
  margin-bottom:60rpx;
}
.golist{
  width:386rpx;
  height:78rpx;
  line-height:78rpx;
  background:#62dfa3;
  color:#fff;
  margin:auto;
  font-size:32rpx;
  border-radius:10rpx;
}