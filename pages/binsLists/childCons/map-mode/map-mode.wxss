map{
 width: 100%;
 height: 50%;
}
.map-element {
  position: fixed;
  bottom: 0;
  width: 100%;
  height: 50%;
  background-color: #fff;
}
.map-element:after{ 
  position:absolute; 
  left:0; 
  right:0; 
  top:-50rpx; 
  content:""; 
  z-index:-1; 
  height:120rpx; 
  width:100%; 
  border-radius:50%; 
  background:#fff;
  }
  .nav_icon {
    position: absolute;
    width: 170rpx;
    height: 170rpx;
    border-radius: 50%;
    background-color: #fff;
    text-align: center;
    left: 40%;
    top: -20%;
  }
  .nav_icon image {
    margin: auto;
    margin-top: 15%;
    width: 96rpx;
    height: 96rpx;
  }
  .nav_icon_text {
    height:25px;
    font-size:24rpx;
    font-weight:400;
    color:rgba(90,219,158,1);
    line-height:53rpx;
  }
  .community-info-top{
    margin-top: 75rpx;
  }
  .community-info-top  image{
    margin-right: 24rpx;
    width: 30rpx;
  }
  .info-top-part {
    display: flex;
    justify-content: flex-start;
    margin-bottom: 34rpx;
    padding: 5rpx;
  }
  .info-top-part  view{
    font-size:30rpx;
    font-weight:300;
    color:rgba(0,0,0,1);
    line-height:30rpx;
  }
  .community-info-bottom {
    border: 0rpx;
    border-top: 1px solid #f7f7f7;
    display: flex;
    justify-content: space-between;
  }
  .community-bottom-part {
    position: relative;
    flex: 1;
    margin: 35rpx 28rpx;
    text-align: center;
  }
  .community-bottom-part::after {
    position: absolute;
    content: '';
    height: 100%;
    width: 1px;
    border: 0rpx;
    border-right: 1px dashed #e5e5e5;
    top: 0;
    right: -16%;
  }
  .bottom-img {
    display: flex;
    justify-content: space-between;
  }
  .bottom-img-box {
    flex: 1;
    text-align: center;
  }
  .bottom-img image {
    margin: auto;
    width: 63rpx;
    height: 63rpx;
  }
  .bottom-img-name {
    font-size: 23rpx;
    font-weight:300;
    color:rgba(0,0,0,1);
    line-height:30px;
    opacity:0.4;
  }
  .bottom-info image {
    position: relative;
    left: 30%;
    width: 86rpx;
    height: 30rpx;
  }
  .bottom-info-name {
    font-size:36rpx;
    font-weight:400;
    color:rgba(0,0,0,1);
    line-height:30px; 
    opacity:0.8;
  }
  .bottom-info-price {
    font-size:30rpx;
    font-weight:400;
    color:rgba(0,0,0,1);
    line-height:30px;
    opacity:0.4;
  }